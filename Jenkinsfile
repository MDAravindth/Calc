pipeline{
	agent any{
		stages{
			stage('Build'){
				step{
					echo"Building"
					mvn clean package
				}
			}
			stage('test'){
				step{
					echo"Building"
					mvn clean package
				}
			}
			stage('Deploy'){
				step{
					echo"Building"
					mvn clean package
				}
			}
		}
	}
}